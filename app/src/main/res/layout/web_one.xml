<?xml version="1.0" encoding="utf-8"?>


<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appbars"
        android:layout_width="match_parent"
        android:visibility="visible"
        android:layout_height="wrap_content"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

        <com.google.android.material.appbar.MaterialToolbar
            android:id="@+id/toolbars"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"

            app:layout_scrollFlags="scroll|enterAlways"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light" />
    </com.google.android.material.appbar.AppBarLayout>

    <LinearLayout
        android:id="@+id/weblayoutone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="?attr/actionBarSize"
    android:background="@color/colorPrimary"
    android:id="@+id/wtrmrkwebone"
    android:visibility="invisible"
    >
    <ImageView
        android:id="@+id/logoggg"
    android:layout_width="?attr/actionBarSize"
    android:src="@drawable/neew_logo"
        android:layout_marginStart="7dp"
        android:layout_alignParentStart="true"
    android:layout_height="?attr/actionBarSize"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#F0EEEE"
        android:layout_marginTop="9dp"
        android:textAllCaps="true"
        android:textStyle="bold"
        android:fontFamily="sans-serif-condensed-medium"
        android:id="@+id/nbtxtweblay"
        android:layout_centerHorizontal="true"
android:paddingStart="70dp"

        android:text="Download 'NewsBox Telangana' App"
        android:textSize="16sp"/>
    <TextView
        android:layout_width="200dp"
        android:textColor="#f0eeee"
        android:textSize="8sp"
        android:textAlignment="center"
        android:layout_marginTop="4dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/nbtxtweblay"
        android:textAllCaps="true"
        android:fontFamily="sans-serif-condensed-light"
        android:text="For Daily Newspapers and Latest News"/>
    <ImageView
        android:layout_width="98dp"
        android:layout_alignParentEnd="true"
        android:layout_height="30dp"
        android:paddingEnd="-17dp"
        android:layout_alignParentBottom="true"
        android:src="@drawable/play_badge"/>
</RelativeLayout>



        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"


            >


            <WebView
                android:id="@+id/webviewone"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </WebView>

            <fr.castorflex.android.circularprogressbar.CircularProgressBar xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/prg"
                android:layout_width="62dp"
                android:layout_height="62dp"

                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:elevation="2dp"
                android:indeterminate="true"
                app:cpb_color="#FFee44"
                app:cpb_colors="@array/mycolors"
                app:cpb_max_sweep_angle="300"
                app:cpb_min_sweep_angle="10"
                app:cpb_rotation_speed="3.2"
                app:cpb_stroke_width="5dp"
                app:cpb_sweep_speed="1.1" />


            <TextView
                android:id="@+id/sakwel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/colorAccentLight"
                android:fontFamily="sans-serif-condensed"
                android:gravity="center"
                android:paddingTop="2dp"
                android:paddingBottom="2dp"
                android:text="@string/wel_sak"
                android:textAllCaps="true"
                android:textColor="#000a12"
                android:textSize="15sp"
                android:visibility="invisible" />

            <com.google.android.material.card.MaterialCardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="16dp"
                app:cardElevation="3dp"
                android:visibility="invisible"
                android:layout_marginBottom="25dp"
                android:layout_alignParentBottom="true"
                app:cardBackgroundColor="#333232"
                android:background="#333232"
                android:backgroundTint="#333232"
              app:contentPadding="3dp"
                android:layout_centerHorizontal="true"
                android:id="@+id/web_one_txtbelow">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                      android:textSize="13sp"
                        android:paddingStart="6dp"

                       android:fontFamily="sans-serif-condensed-medium"
                        android:text="Shared via NewsBox TS App"


                        android:textColor="#fff"
                        />
                    <ImageView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                        android:layout_marginStart="6dp"
                        android:layout_marginEnd="4dp"
                        android:paddingTop="1dp"
                    android:src="@mipmap/ic_launcher"
                    />
                </LinearLayout>
            </com.google.android.material.card.MaterialCardView>





        </RelativeLayout>
    </LinearLayout>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fabone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom|start"
        android:layout_margin="9dp"
        android:clickable="true"

        android:src="@drawable/ic_share_white_36dp"
        app:fabSize="mini" />
</androidx.coordinatorlayout.widget.CoordinatorLayout>